{
    "name": "spacemade/git",
    "description": "Flysystem Adapter for a GIT Repository",
    "keywords": [
        "spacemade-git",
        "flysystem"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.11 || ^11.0.4"
    },
    "autoload": {
        "psr-4": {
            "SpaceMade\\GIT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpaceMade\\GIT\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
